{
	"name": "electron-react-materialui-boilerplate",
	"productName": "electron-react-materialui-boilerplate",
	"version": "1.0.0",
	"description": "Yet another boilerplate",
	"main": ".webpack/main",
	"scripts": {
		"start": "electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"lint": "echo \"No linting configured\""
	},
	"keywords": [],
	"author": {
		"name": "NathanaÃ«l Spriet",
		"email": "nathanael.spriet@gmail.com"
	},
	"license": "MIT",
	"config": {
		"forge": {
			"packagerConfig": {},
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "test_electron_2"
					}
				},
				{
					"name": "@electron-forge/maker-zip",
					"platforms": [
						"darwin"
					]
				},
				{
					"name": "@electron-forge/maker-deb",
					"config": {}
				},
				{
					"name": "@electron-forge/maker-rpm",
					"config": {}
				}
			],
			"plugins": [
				[
					"@electron-forge/plugin-webpack",
					{
						"mainConfig": "./webpack.main.config.js",
						"renderer": {
							"config": "./webpack.renderer.config.js",
							"entryPoints": [
								{
									"html": "./src/client/index.html",
									"js": "./src/client/renderer.js",
									"name": "main_window"
								}
							]
						}
					}
				]
			]
		}
	},
	"devDependencies": {
		"@babel/core": "^7.14.0",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/preset-react": "^7.13.13",
		"@electron-forge/cli": "^6.0.0-beta.54",
		"@electron-forge/maker-deb": "^6.0.0-beta.54",
		"@electron-forge/maker-rpm": "^6.0.0-beta.54",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.54",
		"@electron-forge/maker-zip": "^6.0.0-beta.54",
		"@electron-forge/plugin-webpack": "6.0.0-beta.54",
		"@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
		"babel-loader": "^8.2.2",
		"css-loader": "^4.2.1",
		"electron": "12.0.7",
		"node-loader": "^1.0.1",
		"style-loader": "^1.2.1"
	},
	"dependencies": {
		"@material-ui/core": "^4.11.3",
		"@material-ui/icons": "^4.11.2",
		"classnames": "^2.2.6",
		"electron-squirrel-startup": "^1.0.0",
		"i18next": "^19.0.0",
		"i18next-browser-languagedetector": "^6.0.0",
		"i18next-xhr-backend": "^3.2.0",
		"mdi-material-ui": "^6.21.0",
		"react": "^16.0.0",
		"react-dom": "^16.0.0",
		"react-i18next": "^11.0.0",
		"react-router-dom": "^5.2.0"
	}
}
